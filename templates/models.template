            yield put({
                type: 'success',
                payload: {
                    merAliMessageTempDeleteResult: testRes
                }
            });
            callback && callback(testRes);
        },
        *merAliMessageTempDetail({ payload, callback }, { call, put }) {
            const testRes = yield call({componentName}.merAliMessageTempDetail, payload);
            yield put({
                type: 'success',
                payload: {
                    merAliMessageTempDetailResult: testRes
                }
            });
            callback && callback(testRes);
        },
        *merAliMessageTempEnable({ payload, callback }, { call, put }) {
            const testRes = yield call({componentName}.merAliMessageTempEnable, payload);
            yield put({
                type: 'success',
                payload: {
                    merAliMessageTempEnableResult: testRes
                }
            });
            callback && callback(testRes);
        }
    },
    reducers: {
        success(state, { payload }) {
            return {
                ...state,
                ...payload
            }
        }
    }
}