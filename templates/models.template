import * as {componentName} from '../services/{componentName}';

export default {
    namespace: '{componentName}',
    state: {},
    effects: {
        *merAliMessageTempList({ payload, callback }, { call, put }) {
            const testRes = yield call({componentName}.merAliMessageTempList, payload);
            yield put({
                type: 'success',
                payload: {
                    merAliMessageTempListResult: testRes
                }
            });
            callback && callback(testRes);
            return testRes;
        },
        *merAliMessageTempEdit({ payload, callback }, { call, put }) {
            const testRes = yield call({componentName}.merAliMessageTempEdit, payload);
            yield put({
                type: 'success',
                payload: {
